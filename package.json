{
  "name": "vue2-simple-carousel",
  "version": "0.3.4",
  "main": "dist/vuesimplecarousel.umd.js",
  "module": "dist/vuesimplecarousel.esm.js",
  "unpkg": "dist/vuesimplecarousel.min.js",
  "browser": {
    "./sfc": "src/Carousel.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oleksii14/vue2-simple-carousel.git"
  },
  "keywords": [
    "vue2",
    "vue",
    "vuejs",
    "carousel",
    "simple",
    "slider",
    "swiper"
  ],
  "author": "Oleksii Shaposhnikov <oshap1044@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Oleksii14/vue2-simple-carousel/issues"
  },
  "homepage": "https://github.com/Oleksii14/vue2-simple-carousel#readme",
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd --file dist/vuesimplecarousel.umd.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es --file dist/vuesimplecarousel.esm.js",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife --file dist/vuesimplecarousel.min.js"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue2-touch-events": "^2.2.1",
    "vue-property-decorator": "^8.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-typescript": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "rollup": "^1.14.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  }
}
